
@{
    ViewData["Title"] = "Success";
}

<h1>Success</h1>
<div class="container">
    <div class="row">
        <div class="col-md-6 offset-md-3 mt-5">
            <div class="card">
                <div class="card-body">
                    <h2 class="card-title text-center mb-4">Registration Successful</h2>
                    <div class="table-responsive">
                        <table class="table table-bordered">
                            <tbody>
                                <tr>
                                    <th>Username:</th>
                                    <td>@Model.Username</td>
                                </tr>
                                <tr>
                                    <th>Email:</th>
                                    <td>@Model.Email</td>
                                </tr>
                                <!-- Add more rows for other user information if needed -->
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>


